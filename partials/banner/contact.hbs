<div class="banner">
	<div class="banner-wrap">
		<div class="banner-inner">
			<div class="banner-content">
				<div class="banner-content-inner clearfix">
					<div class="welcome-text">
						<h1>{{{title}}}</h1>
						<div class="circle main-color">				
							{{> misc/social-icons}}
						</div>
					</div>					
				</div>
			</div>
		</div>
	</div>
	{{#if image}}
	<div class="background" style="background-image: url({{image}})"
		data-bottom-top="transform: translate3d(0px,-200px, 0px);"
		data-top-bottom="transform: translate3d(0px,200px, 0px);"
		data-center="transform: translate3d(0px,0px, 0px);">
	</div>
	{{else}}
	<div class="gmap-container">
		<div class="gmap" id="gmap"
			data-latitude="10.839776" {{!Change Your map latitude here}}
			data-longitude="106.647159" {{!Change Your map longitude here}}
		>
		</div>
	</div>
	{{/if}}
	<div class="more-detail">
		<div class="scrollDown">
			<span class="mouseBorder"></span>
		    <span class="mouseWheel"></span>
		</div>
	</div>
</div>